{
  "name": "@raita/raita",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:finnishtransportagency/raita.git",
  "author": {},
  "license": "EUPL-1.2",
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "s3:ls": "aws s3 ls $RAITA_DATA_S3",
    "s3:put:file:meeri": "aws s3 cp $RAITA_TEST_DATA_ROOT/single-files/$npm_config_file s3://$RAITA_DATA_S3/Meeri",
    "s3:put:local:folder:meeri": "aws s3 cp --recursive --exclude \"**.DS_Store\" $RAITA_TEST_DATA_ROOT/folders/$npm_config_folder s3://$RAITA_DATA_S3/Meeri",
    "s3:put:cloud:folder:meeri": "aws s3 cp --recursive  s3://$RAITA_TEST_DATA_BUCKET/$npm_config_folder s3://$RAITA_DATA_S3/Meeri",
    "parser:logs:tail": "aws logs tail $RAITA_LOG_GROUP_PATH --follow --region $RAITA_REGION",
    "pipeline:deploy": "cross-env ENVIRONMENT=$npm_config_environment BRANCH=$npm_config_branch STACK_ID=$npm_config_stackid cdk --app \"ts-node bin/raita-pipeline\" deploy stack-pipeline-raita-$npm_config_stackid",
    "pipeline:synth": "cross-env ENVIRONMENT=$npm_config_environment BRANCH=$npm_config_branch STACK_ID=$npm_config_stackid cdk synth"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "^2.43.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.10",
    "husky": "^8.0.1",
    "jest": "^26.4.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-elasticsearch": "^1.173.0",
    "@aws-cdk/custom-resources": "^1.173.0",
    "@aws-sdk/client-opensearch": "^3.171.0",
    "@aws-sdk/credential-provider-node": "^3.171.0",
    "@opensearch-project/opensearch": "^2.0.0",
    "aws-cdk-lib": "^2.51.1",
    "aws-lambda": "^1.0.7",
    "aws-opensearch-connector": "^1.1.0",
    "constructs": "^10.1.106",
    "date-fns": "^2.29.3",
    "zod": "^3.19.1"
  }
}
