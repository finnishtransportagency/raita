{
  "name": "@raita/raita",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:finnishtransportagency/raita.git",
  "author": {},
  "license": "EUPL-1.2",
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npx --node-options='--max-old-space-size=4096' jest --runInBand",
    "cdk": "cdk",
    "s3:ls": "aws s3 ls $RAITA_DATA_S3",
    "s3:put:file:meeri": "aws s3 cp $RAITA_TEST_DATA_ROOT/single-files/$npm_config_file s3://$RAITA_DATA_S3/Meeri",
    "s3:put:local:folder:meeri": "aws s3 cp --recursive --exclude \"**.DS_Store\" $RAITA_TEST_DATA_ROOT/folders/$npm_config_folder s3://$RAITA_DATA_S3/Meeri",
    "s3:put:cloud:folder:meeri": "aws s3 cp --recursive  s3://$RAITA_TEST_DATA_BUCKET/$npm_config_folder/ s3://$RAITA_DATA_S3/Meeri",
    "s3:put:cloud:file:to-reception": "aws s3 cp s3://$RAITA_TEST_DATA_BUCKET/$npm_config_folder/$npm_config_file s3://$RAITA_RECEPTION_S3/Meeri/2022/TestKamppis/20220202/$npm_config_file",
    "parser:logs:tail": "aws logs tail $RAITA_LOG_GROUP_PATH --follow --region $RAITA_REGION",
    "pipeline:deploy": "cross-env ENVIRONMENT=$npm_config_environment BRANCH=$npm_config_branch STACK_ID=$npm_config_stackid cdk --app \"ts-node bin/raita-pipeline\" deploy stack-pipeline-raita-$npm_config_stackid",
    "pipeline:synth": "cross-env ENVIRONMENT=$npm_config_environment BRANCH=$npm_config_branch STACK_ID=$npm_config_stackid cdk synth",
    "prisma:create-schema": "./create-prisma-schema.sh",
    "prisma:generate": "prisma generate --schema=backend/db/prisma/schema.prisma",
    "graphql:codegen": "graphql-codegen --config codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@types/archiver": "^6.0.2",
    "@types/aws-lambda": "^8.10.130",
    "@types/cloneable-readable": "^2.0.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/lodash": "^4.17.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.4",
    "@types/unzipper": "^0.10.9",
    "aws-cdk": "^2.177.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.9",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prisma": "^5.12.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.3",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-sdk/client-codepipeline": "^3.731.1",
    "@aws-sdk/client-ecs": "^3.731.1",
    "@aws-sdk/client-lambda": "^3.731.1",
    "@aws-sdk/client-s3": "^3.731.1",
    "@aws-sdk/client-secrets-manager": "^3.731.1",
    "@aws-sdk/client-ses": "^3.731.1",
    "@aws-sdk/client-sqs": "^3.731.1",
    "@aws-sdk/client-ssm": "^3.731.1",
    "@aws-sdk/credential-provider-node": "^3.731.1",
    "@aws-sdk/lib-storage": "^3.731.1",
    "@aws-sdk/s3-request-presigner": "^3.731.1",
    "@prisma/client": "^5.12.1",
    "@side/jest-runtime": "^1.1.0",
    "archiver": "^6.0.1",
    "aws-cdk-lib": "^2.177.0",
    "axios": "^1.6.2",
    "cloneable-readable": "^3.0.0",
    "constructs": "^10.3.0",
    "csv-string": "^4.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "graphql": "^16.8.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "npm": "^11.0.0",
    "pino": "^8.16.2",
    "pino-lambda": "^4.4.0",
    "unzipper": "^0.10.14",
    "zod": "^3.22.4",
    "zod-csv": "file:./external/zod-csv"
  }
}
